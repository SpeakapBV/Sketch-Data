{
  "name": "speakapdata",
  "version": "0.1.04",
  "description": "Plugin for randomly creating data like usernames, job titles etc.",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Speakap Data",
    "manifest": "src/manifest.json",
    "main": "speakapdata.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SpeakapBV/Sketch-Data.git"
  },
  "author": "Dennis de jong <dennis.jong@speakap.com>"
}
